#include <xil_io.h>

#define TIMER_ADDRESS 0x41C00000
#define GPIO_ADDRESS  0x40000000

int main()
{
	// AXI Timer configuration
	Xil_Out32(TIMER_ADDRESS, 0x90);

	while(1)
	{
		u32 t = Xil_In32(TIMER_ADDRESS + 0x08);
		Xil_Out32(GPIO_ADDRESS, t);
	}
}


